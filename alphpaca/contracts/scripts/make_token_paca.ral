TxScript Buildtokenpaca (
    contract: CreateToken,
    symbol: ByteVec,
    name: ByteVec,
    decimals: U256,
    tokenTotal: U256
) {
    let pacaFee = contract.getPacaFee()
    let minimalAlphInContract = 1 alph
    let approvedAlphAmount = minimalAlphInContract

    contract.buildtoken{callerAddress!() -> ALPH: approvedAlphAmount, contract.getTokenId(): pacaFee}(symbol, name, decimals, tokenTotal)
}